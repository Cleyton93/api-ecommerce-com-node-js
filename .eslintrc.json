{
    "env": {
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
    "class-methods-use-this": "off",
    "no-param-reassign": "off",
    "no-shadow": "off",
    "func-names": "off",
    "no-underscore-dangle": "off",
    "import/extensions": "off",
    "import/no-unresolved": "off"
    },
    "plugins": [
      "prettier"
    ]
}
